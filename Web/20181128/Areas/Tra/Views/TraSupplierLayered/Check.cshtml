@{
    ViewBag.Title = "Check";
    Layout = "~/Views/Shared/_Home.cshtml";
}

<form class="layui-form layui-form-pane" action="/Tra/TraSupplierLayered/EditTraSupplierLayered" method="post">
    <div class="layui-card">
        <div class="layui-card-header">运输供应商信息</div>
        <div class="layui-card-body" style="overflow:hidden;"> 
            <div class="layui-form-item" style="width:33%;clear:none !important; float:left;">
                <label class="layui-form-label">供应商名称</label>
                <div class="layui-input-inline">
                    <input id="txtSupplierName" type="text" value="@Model.SupplierName" lay-verify="supplier" class="layui-input" disabled>
                    <input id="hdTransportId" type="hidden" name="TransportId" value="@Model.TransportId" />
                    <input id="hdTraLayeredId" type="hidden" name="TraLayeredId" value="@Model.TraLayeredId" />
                </div>
            </div>
            <div class="layui-form-item" style="width:33%;clear:none !important; float:left;">
                <label class="layui-form-label" style="width:130px;">运输供应商编号</label>
                <div class="layui-input-inline" style="width:170px;">
                    <input id="txtTransportNumber" name="TransportNumber" type="text" value="@Model.TransportNumber" lay-verify="supplier" class="layui-input" disabled>
                </div>
            </div>
        </div>
    </div>
    <div class="layui-card">
        <div class="layui-card-header">运输供应商分层</div>
        <div class="layui-card-body" style="overflow:hidden;"> 
            <div class="layui-form-item" style="width:33%;clear:none !important; float:left;">
                <label class="layui-form-label">分层名称</label>
                <div class="layui-input-inline">
                    <input id="txtLayeredName" name="LayeredName" type="text" value="@Model.LayeredName" lay-verify="supplier" class="layui-input" disabled>
                </div>
            </div>
        </div>
    </div>
    <div class="layui-card">
        <div class="layui-card-header">运输供应商分层变更登记信息</div>
        <div class="layui-card-body" style="overflow:hidden;height:245px;">
            <div class="layui-form-item" style="width:33%;clear:none !important; float:left;">
                <label class="layui-form-label" style="width:130px;">分层开始时间</label>
                <div class="layui-input-inline" style="width:170px;">
                    <input id="txtBeginTime" name="BeginTime" type="text" lay-verify="required" value="@Model.BeginTime" class="layui-input" disabled>
                </div>
            </div>
            <div class="layui-form-item" >
                <label class="layui-form-label">备注</label>
                <div class="layui-input-block"> 
                    <textarea id="txtRemark" name="Remark" lay-verify="required" maxlength="50" class="layui-textarea" disabled>@Model.Remark</textarea>
                </div>
            </div>
            <div class="layui-form-item">
                <div class="layui-input-inline" style="margin-left:350px;"> 
                    <a class="layui-btn" id="btnGoBack">返回</a>
                </div>
            </div>

        </div>
    </div>
</form>
@section scripts{
    <script>
        layui.use(['form', 'element', 'laydate', 'upload'], function () {
            var form = layui.form, ele = layui.element, laydate = layui.laydate;
             
        });

        $(function () {
          
            // 返回
            $(document).on('click', '#btnGoBack', function () {
                location.href = '/Tra/TraSupplierLayered/Index';
            });
        });
 
    </script>
}


